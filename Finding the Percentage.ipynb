   "source": [
    "n = int(input())\n",
    "d = {}\n",
    "for i in range(n):\n",
    "    name, *score = input().split()\n",
    "    score = list(map(float,score))\n",
    "    d[name] = score\n",
    "target_name = input()\n",
    "for name, score in d.items():\n",
    "    if target_name == name:\n",
    "        avg = sum(score)/len(score)\n",
    "print(f'{avg:.2f}')"
   ]
